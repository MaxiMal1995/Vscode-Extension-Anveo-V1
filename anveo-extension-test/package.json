{
  "name": "anveo-extension-v1",
  "displayName": "Anveo Extension v1",
  "description": "Anveo Vscode Extension V1",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [  
    "onView:nodeDependencies",
  "command","anveo.testsaveScript"  
  ],
  "main": "./out/extension.js",
  "contributes": {

    "commands": [
      {
        "command": "anveo.SetConnection",
        "title": "Set Connection"
      },
      {
        "command": "anveo.createWebView",
        "title": "User Login"
      },
      {
        "command": "anveo.testPassword",
        "title": "Get Password"
      },
      {
        "command": "anveo.testsaveScript",
        "title": "Active"
      },
      {
        "command": "anveo.setAuth",
        "title": "Set Authentification"
      },
      {
        "command": "anveo.requestAllScript",
        "title": "Download all Anveo Scripts"
      },
      {
        "command": "anveo.createNewScript",
        "title": "Create New Script/Library/Webview Scripts"
      },
      {
        "command": "anveo.getScriptsName",
        "title": "Show actual script name"
      },{
        "command": "anveo.showScriptLibs",
        "title": "Show Libraries"
      }
    ],
    "explorer": [
      {
        "id": "nodeDependencies",
        "name": "Node Dependencies"
      }
    ],
    "snippets": [
      {
        "language": "lua",
        "path": "./src/snippets/lua.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
